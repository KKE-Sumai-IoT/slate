{
  "attributes": {
    "name": "Backdoor Lock",
    "location_id": "a5cc3e6c-423b-4354-ac40-f7fd5b66c739",
    "default_guest_start_time": "11:15:00",
    "power_source": "alkaline_battery",
    "local_pins": [
